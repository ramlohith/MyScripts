#!/usr/bin/python

import sys

a,b = map(int,raw_input().split())

x = 0
y = 0
z = 0

for i in range(1,7):
        if abs(a-i) < abs(b-i):
                x = x + 1
        elif abs(a-i) > abs(b-i):
                y = y + 1
        else:
                z = z + 1

print str(x) + " " + str(z) + " "+ str(y)
